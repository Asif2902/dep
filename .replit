run = "cowsay Configure me!"
entrypoint = "README.md"
modules = ["web", "nodejs-20"]

[nix]
channel = "stable-25_05"
packages = ["cowsay"]

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Compile Contract"

[[workflows.workflow]]
name = "Compile Contract"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx hardhat compile"

[workflows.workflow.metadata]
outputType = "console"
